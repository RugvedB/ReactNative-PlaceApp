{
  "expo": {
    "name": "Click Places",
    "description":"Hey! This is Click Places app.",
    "slug": "click-places",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/places.png",
    "splash": {
      "image": "./assets/splash_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier":"com.rugved.click-places",
      "buildNumber":"1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package":"com.rugved.click_places",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage":"./assets/places-adaptive.png",
        "backgroundColor": "#171717"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION","CAMERA","WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
